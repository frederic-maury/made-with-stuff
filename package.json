{
  "name": "@mfrederic/made-with-stuff",
  "version": "1.0.3",
  "description": "A simple, fun Vue.js (and React using Vuera) component to display a customisable 'made with ‚ù§ by someone' tag",
  "main": "./dist/@mfrederic/made-with-stuff.umd.js",
  "unpkg": "./dist/@mfrederic/made-with-stuff.umd.min.js",
  "scripts": {
    "dev": "vue serve ./src/MadeWithStuff.vue",
    "build": "vue-cli-service build --target lib --name @mfrederic/made-with-stuff ./src/MadeWithStuff.vue && cp ./src/made-with-stuff.d.ts ./dist/@mfrederic/made-with-stuff.d.ts && cp ./package.json ./dist/package.json",
    "build-bundle": "vue-cli-service build --target lib --name @mfrederic/made-with-stuff ./src/MadeWithStuff.vue && cp ./src/made-with-stuff.d.ts ./dist/@mfrederic/made-with-stuff.d.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "MeStrak",
  "license": "MIT",
  "private": false,
  "types": "./dist/@mfrederic/made-with-stuff.d.ts",
  "files": [
    "dist",
    "dist/*",
    "dist/**/*",
    "*.js",
    "*.css",
    "*.d.ts"
  ],
  "dependencies": {
    "vue": "2.6.14"
  },
  "devDependencies": {
    "@vue/cli": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "vue-template-compiler": "2.6.14"
  }
}
